package CollectionDemo;

import java.util.LinkedList;

public class LinkedListDemo {

	public static void main(String[] args) {
		
		LinkedList cities=new LinkedList();
		
		cities.add("jajpur");
		cities.add("puri");
		cities.add(1,"kdp");
		cities.add("kjr");
		
		System.out.println(cities.size());

	}

}
