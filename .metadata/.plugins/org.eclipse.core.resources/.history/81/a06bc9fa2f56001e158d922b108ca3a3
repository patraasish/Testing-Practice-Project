package com.mypratice;

public class Loop_Pratice {

	public static void main(String args[]) {
		
		//while loop
		int i=1;
		while(i<3) {
			System.out.println("I am in While loop");
			i++;
		}
		i=1;
		//do while loop
		do {
			System.out.println("I am in do while loop");
			
		}while(i>2);
		
		
		
	}
	
}
